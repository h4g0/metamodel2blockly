<?xml version='1.0' encoding='UTF-8'?>
<devops2:Pipeline xmlns:xmi="http://www.omg.org/XMI" xmlns:devops2="http://www.example.org/devops2" Name="" xmi:version="2.0">
  <tool Name="language: android&#10;"/>
  <when Trigger="only:&#10;- master&#10;- develop&#10;"/>
  <job Name="before_install">
    <command Name="">
      <tool Name="date"/>
    </command>
    <command Name="" depends="//@job.0/@command.0">
      <tool Name="./envsetup 1&gt; /dev/null 2%&gt;1"/>
    </command>
    <command Name="" depends="//@job.0/@command.1">
      <tool Name="cd $TRAVIS_BUILD_DIR"/>
    </command>
    <command Name="" depends="//@job.0/@command.2">
      <tool Name="cd tools/travis-ci"/>
    </command>
    <command Name="" depends="//@job.0/@command.3">
      <tool Name="sh setupNDK.sh"/>
    </command>
    <command Name="" depends="//@job.0/@command.4">
      <tool Name="export PATH=`pwd`/android-ndk:$PATH"/>
    </command>
    <command Name="" depends="//@job.0/@command.5">
      <tool Name="echo $PATH"/>
    </command>
    <command Name="" depends="//@job.0/@command.6">
      <tool Name="cd $TRAVIS_BUILD_DIR"/>
    </command>
  </job>
  <job Name="script">
    <command Name="">
      <tool Name="cd $TRAVIS_BUILD_DIR"/>
    </command>
    <command Name="" depends="//@job.1/@command.0">
      <tool Name="ndk-build -j8"/>
    </command>
  </job>
  <job Name="after_script">
    <command Name="">
      <tool Name="date"/>
    </command>
  </job>
</devops2:Pipeline>
