  </job>
  <job Name="after_script">
    <command Name="">
      <tool Name="~/_tools/travis/travis test"/>
    </command>
  </job>
  <when Trigger="only:&#10;- master&#10;"/>
  <environment Key="TOOL=&quot;gcc&quot;" Value="TOOL=&quot;gcc&quot;"/>
  <environment Key="TOOL=&quot;clang&quot;" Value="TOOL=&quot;clang&quot;"/>
  <environment Key="TOOL=&quot;i686-w64-mingw32&quot;" Value="TOOL=&quot;i686-w64-mingw32&quot;"/>
