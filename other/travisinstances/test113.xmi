<?xml version='1.0' encoding='UTF-8'?>
<devops2:Pipeline xmlns:xmi="http://www.omg.org/XMI" xmlns:devops2="http://www.example.org/devops2" Name="" xmi:version="2.0">
  <tool Name="git:&#10;    depth: false&#10;    quiet: false&#10;"/>
  <tool Name="language: generic&#10;"/>
  <tool Name="dist: bionic&#10;"/>
  <tool Name="sudo: required&#10;"/>
  <tool Name="jobs:&#10;    include:&#10;        if: commit_message =~ /(deploy-yes)/&#10;        script:&#10;        - chmod u+x push-image-to-nexus.sh &amp;&amp; bash push-image-to-nexus.sh&#10;        - chmod u+x deploy_sdk.sh &amp;&amp; bash deploy_sdk.sh&#10;        - chmod u+x publish-npm.sh &amp;&amp; bash publish-npm.sh&#10;"/>
  <tool Name="notifications:&#10;    email:&#10;        recipients:&#10;        - harasunu.narayan@froala.com&#10;        on_success: always&#10;        on_failure: always&#10;"/>
  <when Trigger="only:&#10;- /dev*/&#10;- /AO-dev*/&#10;- /QA*/&#10;- /QE*/&#10;- /RC*/&#10;- /Release-Master*/&#10;"/>
  <job Name="env">
    <command Name="">
      <tool Name="SHORT_COMMIT= `git rev-parse --short=7 ${TRAVIS_COMMIT}`"/>
    </command>
  </job>
  <job Name="before_install">
    <command Name="">
      <tool Name="echo $TRAVIS_BRANCH"/>
    </command>
    <command Name="" depends="//@job.1/@command.0">
      <tool Name="echo $PWD"/>
    </command>
    <command Name="" depends="//@job.1/@command.1">
      <tool Name="echo $TRAVIS_COMMIT"/>
    </command>
    <command Name="" depends="//@job.1/@command.2">
      <tool Name="echo $BUILD_REPO_NAME"/>
    </command>
  </job>
  <environment Key="SHORT_COMMIT= `git rev-parse --short=7 ${TRAVIS_COMMIT}`" Value="SHORT_COMMIT= `git rev-parse --short=7 ${TRAVIS_COMMIT}`"/>
</devops2:Pipeline>
