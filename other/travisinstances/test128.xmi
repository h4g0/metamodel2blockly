<?xml version='1.0' encoding='UTF-8'?>
<devops2:Pipeline xmlns:xmi="http://www.omg.org/XMI" xmlns:devops2="http://www.example.org/devops2" Name="" xmi:version="2.0">
  <tool Name="language: go&#10;"/>
  <tool Name="sudo: false&#10;"/>
  <tool Name="go:&#10;- 1.12.x&#10;- 1.13.x&#10;- tip&#10;"/>
  <tool Name="matrix:&#10;    allow_failures:&#10;    -   go: tip&#10;"/>
  <when Trigger="only:&#10;- master&#10;- develop&#10;- travis&#10;"/>
  <job Name="before_install">
    <command Name="">
      <tool Name="if [ -n &quot;$GH_USER&quot; ]; then git config --global github.user ${GH_USER}; fi;"/>
    </command>
    <command Name="" depends="//@job.0/@command.0">
      <tool Name="if [ -n &quot;$GH_TOKEN&quot; ]; then git config --global github.token ${GH_TOKEN}; fi;"/>
    </command>
    <command Name="" depends="//@job.0/@command.1">
      <tool Name="go get github.com/mattn/goveralls"/>
    </command>
  </job>
  <job Name="before_script">
    <command Name="">
      <tool Name="make deps"/>
    </command>
  </job>
  <job Name="script">
    <command Name="">
      <tool Name="make qa"/>
    </command>
  </job>
  <job Name="after_failure">
    <command Name="">
      <tool Name="cat ./target/test/report.xml"/>
    </command>
  </job>
  <job Name="after_success">
    <command Name="">
      <tool Name="if [ &quot;$TRAVIS_GO_VERSION&quot; = &quot;1.9&quot; ]; then $HOME/gopath/bin/goveralls -covermode=count -coverprofile=target/report/coverage.out -service=travis-ci; fi;"/>
    </command>
  </job>
</devops2:Pipeline>
